{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "bunVersion": "1.3.1",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/index.ts"
    }
  ],
  "functions": {
    "src/index.ts": {
      "includeFiles": "node_modules/@tensorflow/tfjs-backend-wasm/dist/*.wasm"
    }
  }
}
